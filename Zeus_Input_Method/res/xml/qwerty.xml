<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >
    
    <Row>
        <Key android:codes="49" android:keyWidth="30%p" android:keyLabel="1" android:keyEdgeFlags="left"/>
        <Key android:codes="50" android:keyWidth="30%p" android:keyLabel="2"/>
        <Key android:codes="51" android:keyWidth="40%p" android:keyLabel="3"/>
    </Row>
    
    <Row>
        <Key android:codes="52" android:keyWidth="30%p" android:keyLabel="4"/>
        <Key android:codes="53" android:keyWidth="30%p" android:keyLabel="5"/>
        <Key android:codes="54" android:keyWidth="40%p" android:keyLabel="6"/>
    </Row>
    
    <Row>
        <Key android:codes="55" android:keyWidth="30%p" android:keyLabel="7"/>
        <Key android:codes="56" android:keyWidth="30%p" android:keyLabel="8"/>
        <Key android:codes="57" android:keyWidth="40%p" android:keyLabel="9"/>
    </Row>
    
    <Row>
         <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" 
                android:keyWidth="30%p" android:keyEdgeFlags="left"/>
        <Key android:codes="48" android:keyWidth="30%p" android:keyLabel="0"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" 
                android:keyWidth="40%p" android:keyEdgeFlags="right"
                android:isRepeatable="true"/>
    </Row>
</Keyboard>
    